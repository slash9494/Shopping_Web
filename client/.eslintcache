s: any) {\n  const [inputs, setInputs] = useState({\n    email: \"\",\n    password: \"\",\n  });\n  const { email, password } = inputs;\n\n  const onChange = (e: ChangeEvent<HTMLInputElement>) => {\n    const { value, name } = e.target;\n    setInputs({\n      ...inputs,\n      [name]: value,\n    });\n  };\n\n  const dispatch = useDispatch();\n  const { loginInfo } = useSelector((state: RootState) => state.userReducer);\n\n  const onSubmit = (e: FormEvent<HTMLFormElement>) => {\n    e.preventDefault();\n\n    try {\n      dispatch<any>({\n        type: LOG_IN_REQUEST,\n        data: {\n          email: email,\n          password,\n        },\n      });\n    } catch (error) {\n      console.log(error);\n    }\n  };\n  useEffect(() => {\n    if (loginInfo?.loginSuccess === true) {\n      props.history.push(\"/\");\n    }\n    if (loginInfo?.loginSuccess === false) {\n      const message = loginInfo.message;\n      alert(message);\n    }\n  }, [loginInfo?.loginSuccess, loginInfo?.message, props.history]);\n  return (\n    <LoginBlock>\n      <Form onSubmit={onSubmit}>\n        <label>Email</label>\n        <input type=\"email\" name=\"email\" onChange={onChange} value={email} />\n        <label>Password</label>\n        <input\n          type=\"password\"\n          name=\"password\"\n          onChange={onChange}\n          value={password}\n        />\n        <br />\n        <Button type=\"submit\">Login</Button>\n      </Form>\n    </LoginBlock>\n  );\n}\n\nexport default withRouter(LoginPage);\n","/Users/lmnlm/Desktop/frontend/Shopping_Web/client/src/hoc/auth.tsx",["199","200","201"],"/Users/lmnlm/Desktop/frontend/Shopping_Web/client/src/modules/index.ts",[],"/Users/lmnlm/Desktop/frontend/Shopping_Web/client/src/modules/types.ts",["202"],"import { ActionType } from \"typesafe-actions\";\nimport * as actions from \"./actions\";\nimport { initialState } from \"./reducers/reducer\";\n\nexport type Action = ActionType<typeof actions>;\n\n// export type UserInfo = {\